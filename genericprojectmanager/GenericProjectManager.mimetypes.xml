<?xml version="1.0"?>

<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="text/x-generic-project">
    <sub-class-of type="text/plain"/>
    <comment>Generic Qt Creator Project file</comment>
    <glob pattern="*.creator"/>
  </mime-type>

  <mime-type type="application/vnd.leancreator.generic.files">
    <sub-class-of type="text/plain"/>
    <comment>Generic Project Files</comment>
    <glob pattern="*.files"/>
  </mime-type>

  <mime-type type="application/vnd.leancreator.generic.includes">
    <sub-class-of type="text/plain"/>
    <comment>Generic Project Include Paths</comment>
    <glob pattern="*.includes"/>
  </mime-type>

  <mime-type type="application/vnd.leancreator.generic.config">
    <sub-class-of type="text/plain"/>
    <comment>Generic Project Configuration File</comment>
    <glob pattern="*.config"/>
  </mime-type>

</mime-info>
